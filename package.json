{
  "name": "audius-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot for Audius music raid campaigns",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && npm run db:generate",
    "start": "npm run build && node dist/app.js",
    "start:bot": "npm run build && node dist/bot.js",
    "start:api": "npm run build && node dist/server.js",
    "start:prod": "node dist/app.js",
    "start:prod:bot": "node dist/bot.js",
    "start:prod:api": "node dist/server.js",
    "dev": "tsx watch src/app.ts",
    "dev:bot": "tsx watch src/bot.ts",
    "dev:api": "tsx watch src/server.ts",
    "dev:js": "nodemon src/app.js",
    "dev:bot:js": "nodemon src/bot.js",
    "dev:api:js": "nodemon src/server.js",
    "deploy": "npm run build && pm2 start ecosystem.config.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "discord",
    "audius",
    "music",
    "bot"
  ],
  "author": "SlashRaid",
  "license": "MIT",
  "dependencies": {
    "@audius/sdk": "^9.1.0",
    "@prisma/client": "^6.12.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "libsodium": "^0.7.15",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "prisma": "^6.12.0",
    "spotify-web-api-node": "^5.0.2",
    "uuid": "^9.0.1",
    "web3": "^4.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/helmet": "^0.0.48",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
