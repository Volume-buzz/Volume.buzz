{
  "name": "audius-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot for Audius music raid campaigns",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:bot": "node src/bot.js",
    "start:api": "node src/server.js",
    "start:railway": "npx prisma db push --accept-data-loss && npx prisma generate && node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:bot": "nodemon src/bot.js",
    "dev:api": "nodemon src/server.js",
    "deploy": "pm2 start ecosystem.config.js",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "discord",
    "audius",
    "music",
    "bot"
  ],
  "author": "SlashRaid",
  "license": "MIT",
  "dependencies": {
    "@audius/sdk": "^9.1.0",
    "@prisma/client": "^6.12.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "libsodium": "^0.7.15",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "prisma": "^6.12.0",
    "uuid": "^9.0.1",
    "web3": "^4.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
